{
  "name": "moa-gen",
  "private": true,
  "main": "src/index.js",
  "repository": "https://github.com/fmal/moa-gen.git",
  "scripts": {
    "start": "micro",
    "dev": "micro-dev",
    "format": "prettier --write \"**/*.{js,md}\"",
    "test": "jest",
    "deps": "npm-check -s",
    "deps:update": "npm-check -u"
  },
  "author": "Filip Malinowski <filip@fmal.me>",
  "license": "Unlicense",
  "dependencies": {
    "micro": "9.3.3",
    "query-string": "6.2.0"
  },
  "devDependencies": {
    "got": "9.3.1",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "micro-dev": "3.0.0",
    "npm-check": "5.9.0",
    "prettier": "1.14.3",
    "test-listen": "1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "testEnvironment": "node",
    "silent": true,
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "now": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
